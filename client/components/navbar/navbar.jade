link(rel="stylesheet", href="/client/components/navbar/navbar.css")

div#navbar 
  div: a(ui-sref="user main"): img(src="data:image/png;base64,")
  div.nav-right(ng-show="!nav.status._id")
    span: a(ui-sref="login") Login
    span(style="margin-left: 7px; margin-right: 7px;") /
    span: a(ui-sref="user register") Register
  div.nav-right(ng-show="nav.status._id")
    span: a(ng-click="nav.logout()") {{ nav.status.email }} Logout
    br
    a(ui-sref="user edit({ id: nav.status._id })") Edit Profile
